[Service]
ExecStart=
ExecStart=/usr/bin/dockerd \
    --storage-opt=dm.use_deferred_removal=true \
    --storage-opt=dm.use_deferred_deletion=true \
    --storage-opt=dm.metadatadev=/dev/nbd1p1 \
    --storage-opt=dm.datadev=/dev/nbd1p2 \
    --storage-driver=devicemapper \
    --log-driver=journald \
    --iptables=false \
    {{ docker_tls_options }}
